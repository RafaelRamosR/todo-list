{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
      "one-var": "off",
      "linebreak-style": "off",
      "no-undef": "off",
      "no-unused-vars": "off",
      "no-use-before-define": "warn"
    }
}
